{
	"env": {
		"browser": true,
		"es6": true
	},
	"parserOptios": {
		"sourceType": "module"
	},
	"rules": {
		"indent": [
			"error",
			"tab"
		],
		"linebreak-style": [
			"error",
			"unix"
		],
		"quotes": [
			"error",
			"single"
		],
		"semi": [
			"error",
			"always"
		],
		"no-tabs": 0
	},
	"extends": "airbnb-base"
}
